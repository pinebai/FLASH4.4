#	Runtime parameters for a test problem for gravity tree solver configured for
#	1P2I symmetry.


# properties of cylinder
sim_temp_c               = 10.0
sim_temp_a               = 300.0
sim_press_a              = 1.0e-14
sim_dens_c               = 3.69250689477e-23


# tolerances for rel. error defined as: |(Phi_num - Phi_anl) / Phi_anl|
sim_solutionErrorTolerance1 = 5.0E-3    # check after initial solve
sim_solutionErrorTolerance2 = 5.0E-3    # check after time step (if any)  


# properties of gases
gamma_1           = 1.0001
gamma             = 1.0001
abar_1            = 2.0
abar_2            = 0.609
eos_singlespeciesa = 0.609
eos_singlespeciesz = 1.0001


#		Grid dimensionality and geometry

geometry = "cartesian"

#		Size of computational volume
#xmin        = -0.3857097e+21 # 500 pc
#xmax        =  0.3857097e+21
#ymin        = -0.3857097e+21
#ymax        =  0.3857097e+21

#xmin        = -1.8803053508e+19 # 500 pc
xmin        = -1.1283e+19
xmax        =  1.1283e+19
ymin        = -5.6415e+18
ymax        =  5.6415e+18
zmin        = -5.6415e+18
zmax        =  5.6415e+18

nblockx     = 2
nblocky     = 1
nblockz     = 1

#  Adaptive Grid refinement parameters
lrefine_max  = 4
lrefine_min  = 4
refine_var_1 = "none"
refine_var_2 = "none"

# field lower limits
smlrho          = 2.4e-28 #2.40544e-30
smallp          = 1e-30 #1e-30
smalle          = 1e-99 #1.0 # 6.236e8 # ~ 10 K
smallt          = 9.99
smallu          = 1e-99 #1e-5

#		Boundary conditions
xl_boundary_type = "periodic"
xr_boundary_type = "periodic"
yl_boundary_type = "reflect"
yr_boundary_type = "reflect"
zl_boundary_type = "reflect"
zr_boundary_type = "reflect"

#	Simulation time, I/O  parameters
nriem   = 1000
cfl		= 0.8
basenm          = "cylinder_"
restart         = .false.


#	checkpoint file output parameters
checkpointFileIntervalTime = 7.8892315e+15 # 250 Myr
checkpointFileIntervalStep = 0
checkpointFileNumber = 0

#	plotfile output parameters
plotfileIntervalTime = 3.1556926e+11 # 1 Myr
plotfileIntervalStep = 0
plotfileNumber = 0

nend            = 0
tmax            = 6.0556926e+13 # 10 Myr
dtinit          = 3.1556926e+7
dtmax           = 3.5778463e+11 # 0.5 Myr
dtmin           = 3.1556926e+9  # 0.001 Myr

log_file        = "cylinder.log"
eintSwitch      = 0 # 1.e-4
run_comment     = "test cylinder"

plot_var_1 = "dens"
plot_var_2 = "pres"
plot_var_3 = "temp"
plot_var_4 = "velx"
plot_var_5 = "vely"
plot_var_6 = "velz"
plot_var_7 = "gpot"
plot_var_8 = "fld1"
plot_var_9 = "fld2"
plot_var_10 = "panl"
plot_var_11 = "perr"


# tree solver
useGravity          = .true.
updateGravity       = .true.
gr_bhPhysMACTW           = .false.
gr_bhPhysMACComm         = .false.
gr_bhTreeLimAngle        = 0.5
gr_bhTreeSafeBox         = 1.2
gr_bhUseUnifiedTW        = .false.
gr_bhTWMaxQueueSize      = 10000

# Gravity
grv_bhNewton            = -1.0
grv_bhMAC               = "ApproxPartialErr"
grv_bhMPDegree          = 2
grv_bhUseRelAccErr      = .true.
grv_bhAccErr            = 0.0001

grav_boundary_type      = "mixed"
grav_boundary_type_x    = "periodic"
grav_boundary_type_y    = "isolated"
grav_boundary_type_z    = "isolated"

# Gravity Ewald field
grv_bhEwaldSeriesN        = 10
grv_bhEwaldAlwaysGenerate = .true.
grv_bhEwaldFieldNxV42        = 32
grv_bhEwaldFieldNyV42        = 32
grv_bhEwaldFieldNzV42        = 32
grv_bhEwaldNRefV42           = -1
grv_bhLinearInterpolOnlyV42  = .true.
grv_bhEwaldFNameAccV42       = "ewald_field_acc"
grv_bhEwaldFNamePotV42       = "ewald_field_pot"
grv_bhEwaldNPer              = 32
grv_bhEwaldFName             = "ewald_coeffs"

# Gravity External gravitational field
grv_useExternalPotential   = .false.
grv_usePoissonPotential    = .true.
grv_bhExtrnPotFile         = "external_potential.dat"
grv_bhExtrnPotType         = "planez"
grv_bhExtrnPotCenterX      = 0.0
grv_bhExtrnPotCenterY      = 0.0
grv_bhExtrnPotCenterZ      = 0.0



